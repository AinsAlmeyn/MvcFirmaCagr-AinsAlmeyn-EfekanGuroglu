
@{
    ViewBag.Title = "cagrıdetay";
    Layout = "~/Views/Shared/FirmaLayout.cshtml";
}

<h3>cagrıdetay</h3>
<br />
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Aciklama</th>
        <th>Tarih</th>
        <th>Saat</th>
        
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.ID</td>
            <td>@x.Aciklama </td>
            <td>@x.Tarih</td>                  
            <td>@x.Saat</td>                  
        </tr>
    }
   </table>